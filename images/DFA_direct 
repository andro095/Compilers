digraph DFA_direct {
	rankdir=LR
	node [shape=circle]
	0 [label=0]
	node [shape=circle]
	1 [label=1]
	node [shape=circle]
	2 [label=2]
	node [shape=doublecircle]
	3 [label=3]
	0 -> 0 [label=b]
	0 -> 1 [label=a]
	1 -> 2 [label=b]
	1 -> 1 [label=a]
	2 -> 3 [label=b]
	2 -> 1 [label=a]
	3 -> 0 [label=b]
	3 -> 1 [label=a]
}
